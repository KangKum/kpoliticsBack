# MongoDB 연결 URI
# 형식: mongodb+srv://<username>:<password>@<cluster>/<database>?retryWrites=true&w=majority
MONGODB_URI=your_mongodb_connection_string_here

# 국회 Open API 키
# 발급: https://open.assembly.go.kr/portal/openapi/main.do
VITE_ASSEMBLY_API_KEY=your_assembly_api_key_here

# 국회 API 기본 URL (일반적으로 변경 불필요)
VITE_API_BASE_URL=https://open.assembly.go.kr/portal/openapi

# 관리자 비밀번호 (게시판 관리용)
ADMIN_PASSWORD=your_admin_password_here

# 공공데이터포털 선거API 키
# 발급: https://www.data.go.kr/
VITE_DATAGO_API_KEY=your_datago_api_key_here

# 프론트엔드 URL (CORS 설정용)
# 개발: http://localhost:3000
# 프로덕션: https://your-vercel-domain.vercel.app
FRONTEND_URL=http://localhost:3000

# 서버 포트 (Render는 자동으로 PORT 환경변수 제공)
PORT=4001
